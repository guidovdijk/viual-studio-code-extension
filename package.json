{
	"name": "handlebars-redirector",
	"publisher": "guido",
	"displayName": "Handlebars redirector",
	"description": "handlebars-redirector is an extention that lets you click on the imported html file statements (e.g '{{> file }}, {{embed 'file' }}' and automatically get redirected to that specific file.",
	"version": "2.1.3",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/guidovdijk/viual-studio-code-extension.git"
	},
	"activationEvents": [
		"onLanguage:html"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^10.2.0",
		"path": "^0.12.7",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	}
}
